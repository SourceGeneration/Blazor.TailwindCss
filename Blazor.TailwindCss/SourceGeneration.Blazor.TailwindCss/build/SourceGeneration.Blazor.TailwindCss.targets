<?xml version="1.0" encoding="utf-8" ?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

	<!--It is generated a target which is executed before the compilation-->
	<Target Name="AfterCompile" AfterTargets="Build">
		<!--Calling our custom task-->
		<TailwindCssGenerateTask ProjectDir="$(RootFolder)" CssOuput="$(RootFolder)/wwwroot"></TailwindCssGenerateTask>
	</Target>
	
</Project>